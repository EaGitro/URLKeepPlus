{
    "manifest_version": 3,
    "name": "URLKeep+",
    "version": "1.0.0.1",
    "description": "an Extension for Google Chrome. Saving URLs and Managing, Arranging them.",
    "permissions": [
        "storage",
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
        "default_icon": "./icon/icon.png"
    },
    "icons":{
        "16": "./icon/icon.png",
        "32": "./icon/icon.png",
        "48": "./icon/icon.png",
        "128": "./icon/icon.png"
    }
}